#include <stdio.h>

int main(void){
    setlocale(LC_CTYPE, "ukr");
    int a, b;
    scanf("%d", &a);
    switch(a){
        case 1:
            printf("понеділок\n");
            break;
        case 2:
            printf("вівторок\n");
            break;
        case 3:
            printf("середа\n");
            break;
        case 4:
            printf("четвер\n");
            break;
        case 5:
            printf("п'ятниця\n");
            break;
        case 6:
            printf("субота\n");
            break;
        case 7:
            printf("неділя\n");
            break;
    }
}
